{
  "name": "ci-workflow",
  "repository": "",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "prepare": "husky",
    "cleanup": "rimraf dist",
    "dev": "vite",
    "build": "yarn prepare && yarn cleanup && tsc && vite build --mode development && yarn check:html",
    "release": "yarn cleanup && tsc && vite build && yarn check:html",
    "lint": "run-s -c 'lint:*'",
    "lint:es": "eslint --fix --ext .ts,.tsx --ignore-pattern .eslintignore . && prettier --write '**/*.{ts,tsx}'",
    "lint:style": "stylelint --fix '**/*.scss'",
    "lint:prettier": "prettier --write '**/*.{js,json,css}'",
    "check:html": "markuplint 'dist/**/*.html'",
    "check:links": "linkinator ./dist --recurse --silent"
  },
  "browserslist": [
    "> 0.2%, not dead",
    "not IE 11",
    "Safari > 15, iOS > 15"
  ],
  "devDependencies": {
    "@types/ua-parser-js": "0.7.39",
    "@typescript-eslint/eslint-plugin": "7.0.1",
    "@typescript-eslint/parser": "7.0.1",
    "autoprefixer": "10.4.17",
    "browserslist": "4.23.0",
    "cpx": "1.5.0",
    "cross-env": "7.0.3",
    "cssnano": "6.0.3",
    "eslint": "8.56.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-typescript-sort-keys": "3.1.0",
    "eslint-plugin-unused-imports": "3.1.0",
    "linkinator": "^6.1.2",
    "lint-staged": "15.2.2",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.35",
    "postcss-calc": "9.0.1",
    "postcss-discard-duplicates": "6.0.1",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-loader": "8.1.0",
    "postcss-preset-env": "9.3.0",
    "postcss-sorting": "8.0.2",
    "prettier": "3.2.5",
    "rimraf": "^6.0.1",
    "sass": "1.71.0",
    "sass-loader": "14.1.0",
    "sharp": "0.33.2",
    "stylelint": "16.2.1",
    "stylelint-config-recommended": "14.0.0",
    "stylelint-config-recommended-scss": "14.0.0",
    "stylelint-config-standard": "36.0.0",
    "stylelint-config-standard-scss": "13.0.0",
    "stylelint-declaration-block-no-ignored-properties": "2.8.0",
    "stylelint-order": "6.0.4",
    "stylelint-scss": "6.1.0",
    "ts-node": "10.9.2",
    "typescript": "5.3.3",
    "vite": "5.1.3",
    "vite-plugin-ejs": "1.7.0",
    "vite-tsconfig-paths": "4.3.1"
  },
  "dependencies": {
    "@markuplint/ejs-parser": "^4.6.16",
    "@markuplint/jsx-parser": "^4.7.16",
    "@markuplint/react-spec": "^4.5.16",
    "@types/node": "^22.5.4",
    "@types/pixi.js": "^5.0.0",
    "axios": "1.6.7",
    "husky": "^9.1.7",
    "markuplint": "^4.11.6",
    "pixi-filters": "^6.0.3",
    "pixi.js": "^8.1.5",
    "swiper": "^11.0.6",
    "tween24": "^1.5.0",
    "vite-plugin-image-optimizer": "^1.1.8",
    "vite-plugin-svgr": "^4.2.0",
    "vitest": "^3.1.2",
    "zod": "^3.24.3"
  }
}
